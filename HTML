$(document).ready(
<script>
var myids = [
    "tintaempixel.wordpress.com/2014/11/21/seacret/",
    "tintaempixel.files.wordpress.com/2014/12/img_0059.jpg",
    "s8.postimg.org/t7zujh9hh/Sra_Do_Destino.jpg",
    "tintaempixel.files.wordpress.com/2015/07/kraken.jpg?w=880",
    "tintaempixel.files.wordpress.com/2015/08/vasalisa.png?w=880",
    "s8.postimg.org/k1hjw7491/Torre.jpg",
    "s8.postimg.org/oyqj3kgud/Inverno.jpg",
    "tintaempixel.files.wordpress.com/2015/08/ny2.png?w=880",
    "s8.postimg.org/hp0d74cw5/Get_Along_World.jpg",
    "tintaempixel.files.wordpress.com/2015/08/ny1.png?w=880",
    "s8.postimg.org/3mj15vww5/3_Deusas.jpg",
    "s8.postimg.org/q3kl8y3b9/La_Vieja.jpg",
    "s8.postimg.org/3jz5j1t8l/caminhosquadrinhos.jpg",
    "tintaempixel.files.wordpress.com/2014/12/img_0035.jpg",
    "s8.postimg.org/4ha65nu4l/Teoria_Critica.jpg",
    "s8.postimg.org/tpqell9ol/Coisas_QJa_Fiz_Em_NYC.jpg",
    "s8.postimg.org/3qcum52dh/Inicial.jpg",
    "tintaempixel.files.wordpress.com/2015/08/tonel.png?w=880",
    "s8.postimg.org/89p37nk91/Foi.jpg",
    "s8.postimg.org/u3rql6ts5/Circulo.jpg"];

currentvwd = null
openInterno = function () {
    // get a random number between 0 and the number of links
    var randIdx = Math.random() * myids.length;
    // round it, so it can be used as array index
    randIdx = parseInt(randIdx, 10);
    // construct the link to be opened
    var nextvwd = myids[randIdx];
    // open it in a new window / tab (depends on browser setting)
    document.getElementById(currentvwd).style.display = "none";
    document.getElementById(nextvwd).style.display = "block";
    currentvwd = nextvwd
};
</script>
)

<button onclick="openInterno();">Interno</button>



